public class Demo
{
	public static void main(String args[])
	{
		int[] arrayOfOneThousand = new int[1000];
		
		for(int i = 0; i < arrayOfOneThousand.length; i++)
		{
			System.out.print(arrayOfOneThousand[i++]);
			
		}
	}
}
